{
  "name": "lms-user-registration",
  "version": "1.0.0",
  "description": "Node.js + Express + MongoDB User Registration System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node scripts/test-registration.js",
    "test:registration": "node scripts/test-registration.js",
    "seed": "node scripts/seed-database.js",
